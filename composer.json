{
    "name": "drupal/ezcontent_demo",
    "description": "A package containing default content used in the demonstration of EzContent profile.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "homepage": "https://drupal.org/project/ezcontent_demo",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "drupal/default_content": "^2.0@alpha"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches":{
            "drupal/default_content":{
                "Do not reimport existing entities":"https://www.drupal.org/files/issues/2022-07-29/default_content-fix-uuid-duplicate-entry-2698425.patch"
            }
        }
    }
}    
